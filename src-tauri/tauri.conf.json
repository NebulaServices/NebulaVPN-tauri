{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "NebulaVPN",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "window": {
        "all": true,
        "create": false
      }
    },

    "bundle": {
      "active": true,
      "category": "vpn",
      "copyright": "",

      "deb": {
        "depends": []
      },

      "externalBin": [],
      "icon": [
        "icons/favicon.ico"
      ],
      "identifier": "org.nebvpn",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "width": 800,
        "minHeight": 600,
        "minWidth": 800,
        "resizable": true,
        "title": "NebulaVPN",
        "decorations": false
      }
    ]
  }
}
