{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "NebulaVPN",
    "version": "0.0.1"
  },
  "tauri": {

    "systemTray": {
      "iconPath": "./icons/Square310x310Logo.png",
      "iconAsTemplate": true,
      "menuOnLeftClick": true
    },

    "allowlist": {
      "all": true,
      "window": {
        "all": true,
        "create": false
      }
    },

    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "nebulaVPN",
      "icon": [
        "./icons/favicon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "exceptionDomain": "",
        "signingIdentity": null,
        "entitlements": null,
        "minimumSystemVersion": ""
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "width": 800,
        "minHeight": 600,
        "minWidth": 800,
        "resizable": true,
        "title": "NebulaVPN",
        "decorations": false,
        "skipTaskbar": true
      }
    ]
  }
}
